{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "Basic setup for react with webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --config build/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config build/webpack.prod.js",
    "build:dev": "webpack --config webpack/webpack.dev.js",
    "build:report": "webpack --config webpack/webpack.report.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "repository": {
